#P1
#Pangram
#Approach - 1
s=input()
s1=set(s)
if len(s1)==27:
    print("Pangram")
else:
    print("Not Pangram")
#Approach - 2
alpha={'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'}
s=input()
d={}
for i in s:
if i in alpha:
    if i not in d:
        d[i]=1
    else:
        d[i]+=1
x=d.keys()
print(d)
if len(x)==26:
    print("Pangram")
else:
    print("Not a pangram")
        
